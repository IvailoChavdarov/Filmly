@model TitleRankingVM
<partial name="_BreadcrumbPartial" />
    <h1>@Model.RankingName</h1>
    <table>
        @foreach (var title in Model.Ranking.Items)
        {
        <tr>
            <td>@title.Title</td>
            <td><a asp-controller="Movies" asp-action="Details" asp-route-id="@title.Id"><img src="@title.Image" alt="Alternate Text" /></a></td>
        </tr>
        }
    </table>
